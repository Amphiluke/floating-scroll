{
  "name": "floating-scroll",
  "version": "2.3.3",
  "description": "A lightweight jQuery plugin providing floating scrollbar functionality",
  "main": "src/jquery.floatingscroll.js",
  "style": "src/jquery.floatingscroll.css",
  "scripts": {
    "lint": "eslint src/jquery.floatingscroll.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "minify": "uglifyjs src/jquery.floatingscroll.js -c -m --comments /^!/ -o src/jquery.floatingscroll.min.js",
    "make-css": "lessc --clean-css=\"--compatibility=ie8 --advanced\" src/jquery.floatingscroll.less src/jquery.floatingscroll.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Amphiluke/floating-scroll.git"
  },
  "keywords": [
    "scrollbar",
    "jquery-plugin",
    "ecosystem:jquery"
  ],
  "author": "Amphiluke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Amphiluke/floating-scroll/issues"
  },
  "homepage": "https://amphiluke.github.io/floating-scroll/",
  "dependencies": {
    "jquery": ">=1.4.3"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "uglify-js": "^3.1.3"
  }
}
